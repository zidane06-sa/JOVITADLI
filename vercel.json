{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "vite-frontend-framework/.next",
  "installCommand": "npm install && cd backend && npm install && cd ..",
  "routes": [
    { "src": "/api/?(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/vite-frontend-framework/$1" }
  ]
}

