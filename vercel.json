{
  "installCommand": "npm install && npm install --prefix backend && npm install --prefix vite-frontend-framework",
  "buildCommand": "npm run build --prefix vite-frontend-framework",
  "outputDirectory": "vite-frontend-framework/.next",
  "routes": [
    { "src": "/api/?(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/vite-frontend-framework/$1" }
  ]
}

